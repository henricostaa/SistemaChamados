                    xmlns:vm="clr-namespace:SistemaChamados.ViewModels"
                    xmlns:view="clr-namespace:SistemaChamados.Views">

    <DataTemplate DataType="x:Type vm:HomeVM">
        <view:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="x:Type vm:TicketsVM">
        <view:TicketsView/>
    </DataTemplate>

    <DataTemplate DataType="x:Type vm:UsuarioVM">
